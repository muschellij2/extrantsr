## The function is currently defined as
function (x, gzipped = TRUE) 
{
    if (inherits(x, "character")) {
        return(x)
    }
    else {
        if (inherits(x, "antsImage")) {
            ext = ".nii"
            if (gzipped) 
                ext = ".nii.gz"
            tfile = paste0(tempfile(), ext)
            antsImageWrite(x, tfile)
            return(tfile)
        }
        else {
            stop("x has unknown class - not char or nifti")
        }
    }
    return(FALSE)
  }
}
